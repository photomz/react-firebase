{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "rules": {
    "indent": ["error", "tab"],
    "no-tabs": 0,
    "react/jsx-indent": ["error", "tab"],
    "react/jsx-indent-props": ["error", "tab"]
  }
}
